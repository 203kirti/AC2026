//Task 1️⃣ : [Love Story](https://codeforces.com/contest/1829/problem/A)

//solutions
import java.util.Scanner;
public class sortarrayofstring {
	public static void main(String []args){
		Scanner sc=new Scanner(System.in);
		int t=sc.nextInt();
		for(int i=0;i<t;i++)
		{
			String str=sc.next();
			String check="codeforces";
	        int c=0;
			for(int j=0;j<10;j++)
			{
				if(str.charAt(j)!=check.charAt(j))
					c++;
			}
			System.out.println(c);
		}
		
	}
 
}
